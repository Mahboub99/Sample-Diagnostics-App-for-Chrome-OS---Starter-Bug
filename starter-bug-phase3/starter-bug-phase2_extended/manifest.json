{
    "manifest_version": 2,
    "name": "starter bug phase 2",
    "version": "0.1.0",
    "description": "chrome extention to fetch system info",

    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon16.png",
            "24": "images/icon24.png",
            "32": "images/icon32.png"
        }
    },
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "system.cpu",
        "system.display",
        "system.storage",
        "system.memory"
    ]
}